{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"],
      "@components/*":["src/components/*"],
      "@constants/*":["src/constants/*"],
      "@context/*":["src/context/*"],
      "@assets/*":["src/assets/*"],
      "@styles/*":["src/styles/*"],
      "@store/*":["src/store/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
